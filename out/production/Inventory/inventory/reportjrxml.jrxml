    <parameter name="inventoryP" class="java.lang.Integer"/>
    <query language="SQL"><![CDATA[SELECT
    s.customer_id AS sales_customer_id,
    s.product_id,
    s.category,
    s.weight,
    s.net_weight,
    s.length,
    s.karat,
    s.gold_rate,
    s.price,
    s.return_value,
    s.date AS sales_date,
    sr.total,
    sr.exchange,
    sr.payable,
    sr.amount,
    sr.balance,
    sr.date AS receipt_date,
    sr.time,
    sr.customer_name,
    sr.customer_tel,
    sr.customer_address,
    sr.payment_method,
    sr.total_weight,
    sr.total_count
FROM
    sales s
INNER JOIN
    sales_receipt sr
ON
    s.customer_id = sr.customer_id
WHERE
    s.customer_id = $P{inventoryP}
]]></query>
